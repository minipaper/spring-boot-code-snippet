{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": ["airbnb-base", "plugin:vue/recommended"],
  "rules": {
    "linebreak-style": "off",
    "import/no-unresolved": "off",
    "no-console": "off",
    "arrow-parens": "off"
  }
}
